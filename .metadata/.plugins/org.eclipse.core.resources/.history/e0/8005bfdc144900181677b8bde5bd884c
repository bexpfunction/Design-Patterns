/**
 * 
 */
package pizza;

/**
 * @author Burak Yazar
 *
 */
public class NYPepperoniPizza extends Pizza {

	/**
	 * 
	 */
	public NYPepperoniPizza() {
		type = pizzaTypes.NYPepperoni;
		name = "New York Double";
		doughType = "Thin";
		toppings.add("Pepperoni");
		toppings.add("Mosarella");
	}

}
