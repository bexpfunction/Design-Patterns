/**
 * 
 */
package character;

import behaviour.SwordBehaviour;

/**
 * @author Burak Yazar
 *
 */
public class Knight extends Character {

	/**
	 * 
	 */
	public Knight() {
		weaponBehaviour = new SwordBehaviour();
	}

	/* (non-Javadoc)
	 * @see character.Character#Fight()
	 */
	@Override
	public void Fight() {
		System.out.println("Knight CHARGE!!!");
		weaponBehaviour.useWeapon();

	}

}
