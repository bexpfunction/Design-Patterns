package character;

import behaviour.SwordBehaviour;

/**
 * @author Burak Yazar
 *
 */
public class King extends Character {

	/**
	 * 
	 */
	public King() {
		weaponBehaviour = new SwordBehaviour();
	}

	/* (non-Javadoc)
	 * @see character.Character#Fight()
	 */
	@Override
	public void Fight() {
		System.out.println("I'am the KING!!!");

	}

}
