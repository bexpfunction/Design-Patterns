/**
 * 
 */
package vehicle;

/**
 * @author Burak Yazar
 *
 */
public abstract class CarComponent extends Vehicle {
	/**
	 * 
	 */

	/* (non-Javadoc)
	 * @see vehicle.Vehicle#getName()
	 */
	@Override
	public abstract String getName();
	/* (non-Javadoc)
	 * @see vehicle.Vehicle#getCost()
	 */
	@Override
	public abstract int getCost();

}
