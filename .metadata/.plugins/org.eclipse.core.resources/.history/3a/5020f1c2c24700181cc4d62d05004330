package vehicle;

/**
 * @author Burak Yazar
 *
 */
public abstract class CarComponent extends Vehicle {

	String name;
	/**
	 * 
	 */
	public CarComponent(String name) {
		this.name = name;
	}

	/* (non-Javadoc)
	 * @see vehicle.Vehicle#getName()
	 */
	@Override
	public abstract String getName();
	/* (non-Javadoc)
	 * @see vehicle.Vehicle#getCost()
	 */
	@Override
	public int getCost() {
		// TODO Auto-generated method stub
		return 0;
	}

}
